<div class="table-responsive" id="hist-reports-table">
	<table class="table table-striped table-bordered table-hover" id="dataTables-example">
		<thead>
			<tr>
				<th>Curso</th>
				<th>Alumnos</th>
				<th>Fecha Reporte</th>
                    <th>Generar</th>
			</tr>
		</thead>
		<tbody>
               <%@reports.each do |report|%>
     			<tr>
     				<td><%=report.coursename%></td>
                         <td>asd</td>
                         <td><%=report.created_at.strftime("%d-%m-%Y")%></td>
                         <td><%=link_to (image_tag "pdf_icon.png", :size => "30x30")%></td>
     			</tr>
               <%end%>
		</tbody>
	</table>
</div>